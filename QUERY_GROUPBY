QUERY 1 - CONTARE QUANTI ISCRITTI CI SONO STATI OGNI ANNO;

  SELECT COUNT(*) AS 'n_studenti', YEAR (`enrolment_date`) AS `anno_accademico` 
  FROM `students` 
  GROUP BY `anno_accademico`; 

